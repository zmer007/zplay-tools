{
  "name": "zplay-tools",
  "version": "0.1.0",
  "main": "main.js",
  "scripts": {
    "pack-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/image/logo/mac.icns --prune=true --out=release-builds",
    "pack-win": "electron-packager . zplay-tools --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/image/logo/window.ico --prune=true --out=release-builds --version-string.CompanyName=ZA --version-string.FileDescription=ZA --version-string.ProductName='Zplay Tools'"
  },
  "devDependencies": {
    "electron": "^1.7.10",
    "electron-packager": "^10.1.1",
    "electron-winstaller": "^2.6.3"
  }
}
